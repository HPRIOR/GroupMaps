{
    "compilerOptions": {
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5",
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}